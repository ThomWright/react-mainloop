{
  "name": "react-mainloop",
  "version": "0.0.0",
  "description": "A React Component that uses a game loop to animate another component.",
  "main": "dist/Animator.js",
  "scripts": {
    "build": "rimraf dist && babel src --out-dir dist --optional es7.objectRestSpread --optional runtime --ignore \"__tests__\"",
    "lint": "eslint src/",
    "jest": "jest src",
    "test": "npm run lint && npm run jest",
    "prepublish": "npm test && npm run build && npm shrinkwrap",
    "postpublish": "rm npm-shrinkwrap.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ThomWright/react-mainloop"
  },
  "keywords": [
    "react",
    "mainloop",
    "main",
    "loop",
    "game",
    "gameloop",
    "animate"
  ],
  "author": "Thom Wright <npm@thomwright.co.uk> (http://thomwright.co.uk/)",
  "license": "EPL-1.0",
  "bugs": {
    "url": "https://github.com/ThomWright/react-mainloop/issues"
  },
  "homepage": "https://github.com/ThomWright/react-mainloop",
  "dependencies": {
    "mainloop.js": "^1.0.0"
  },
  "devDependencies": {
    "babel": "^5.4.7",
    "babel-eslint": "^3.1.9",
    "babel-jest": "^5.2.0",
    "eslint": "^0.21.2",
    "eslint-plugin-react": "^2.3.0",
    "jest-cli": "^0.4.5",
    "rimraf": "^2.3.4"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "testFileExtensions": ["es6", "js", "jsx"],
    "moduleFileExtensions": ["js", "json", "es6", "jsx"],
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-tools"
    ]

  }
}
